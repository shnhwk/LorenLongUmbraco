@inherits Umbraco.Web.Macros.PartialViewMacroPage
@{
    var mediaIds = Model.MacroParameters["Images"];
    var imgToDisplay = 0;
    var url = "";
    if (mediaIds != null)
    {
        var split = mediaIds.ToString().Split(',');
        Random random = new Random();
        int randomNumber = random.Next(0, split.Length);
        imgToDisplay = int.Parse(split[randomNumber]);
        url = Umbraco.Media(@imgToDisplay).url;
    }

}
 
<img src="@url" alt="Loren Long" />
       
         
 